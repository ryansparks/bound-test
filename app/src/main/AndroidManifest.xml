<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.victorluo.boundtestapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SigninActivity" />
        <activity android:name=".CreateaccountActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity android:name=".FriendsActivity" />
        <activity android:name=".MeetingInfoActivity" />
        <activity android:name=".PayPalActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".DateTimeLocationPriceActivity" />
        <activity android:name=".CurrentMeetupsActivity" />
        <activity android:name=".AboutBoundActivity" />
        <activity android:name=".ChangeCardActivity" />
        <activity android:name=".ForgotPasswordActivity"></activity>
    </application>

</manifest>